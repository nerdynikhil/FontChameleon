{
    "manifest_version": 3,
    "name": "FontChameleon",
    "version": "1.0",
    "description": "Replace any font on the webpage with a selected font.",
    "permissions": [
      "activeTab",
      "scripting"
    ],
    "action": {
      "default_popup": "popup.html",
      "default_icon": {
        "16": "icons/icon16.png",
        "48": "icons/icon48.png",
        "128": "icons/icon128.png"
      }
    },
    "background": {
      "service_worker": "background.js"
    }
  }
  